{
    "source": {
        "filter": "*.ico",
        "depth": 8,
        "sizes": [
            {
                "width": 16,
                "height": 16
            },
            {
                "width": 24,
                "height": 24
            },
            {
                "width": 32,
                "height": 32
            },
            {
                "width": 48,
                "height": 48
            },
            {
                "width": 64,
                "height": 64
            },
            {
                "width": 128,
                "height": 128
            },
            {
                "width": 256,
                "height": 256
            }
        ]
    },
    "destination": {
        "sizeDirectoryNamePattern": "{2}",
        "directoryNamePattern": "",
        "fileType": "png",
        "fileNamePattern": "{0}"
    },
    "imageProcessor": {
        "type": "ImageMagickShell",
        "path": "C:\\Program Files\\ImageMagick"
    }
}
